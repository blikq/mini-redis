    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'mini_redis'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=mini_redis",
                    "--package=mini_redis"
                ],
                "filter": {
                    "name": "mini_redis",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in executable 'mini_redis'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--bin=mini_redis",
                    "--package=mini_redis"
                ],
                "filter": {
                    "name": "mini_redis",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug integration test 'test'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--test=test",
                    "--package=mini_redis"
                ],
                "filter": {
                    "name": "test",
                    "kind": "test"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        }
    ]
}